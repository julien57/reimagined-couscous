{% extends 'index.html.twig' %}

{% block content %}

    {% set index = 0  %}

    {% set excluded_template = {} %}

    {%  for key,block in blocks %}

        {% set include = '/home/blocks/' ~ block.template %}
        {% set no_margin_top = '' %}
        {% set even = '' %}

        {% include include with { 'data': block.data, 'even': even, 'index': index } %}

    {%  endfor  %}

{% endblock %}
